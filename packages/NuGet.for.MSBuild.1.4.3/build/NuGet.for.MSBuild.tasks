<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <UsingTask TaskName="NuGet.MSBuild.Tools.Pack" AssemblyFile="$(MSBuildThisFileDirectory).\NuGet.MSBuild.Tools.dll" />
  <UsingTask TaskName="NuGet.MSBuild.Tools.Push" AssemblyFile="$(MSBuildThisFileDirectory).\NuGet.MSBuild.Tools.dll" />
  <UsingTask TaskName="NuGet.MSBuild.Tools.Spec" AssemblyFile="$(MSBuildThisFileDirectory).\NuGet.MSBuild.Tools.dll" />

  <UsingTask TaskName="NuGet.MSBuild.Tools.ReadAssemblyMetadata" AssemblyFile="$(MSBuildThisFileDirectory).\NuGet.MSBuild.Tools.dll"/>
  <UsingTask TaskName="NuGet.MSBuild.Tools.ReadNuspecManifest" AssemblyFile="$(MSBuildThisFileDirectory).\NuGet.MSBuild.Tools.dll"/>
  <UsingTask TaskName="NuGet.MSBuild.Tools.ResolveNuGetToolPath" AssemblyFile="$(MSBuildThisFileDirectory).\NuGet.MSBuild.Tools.dll" />

</Project>